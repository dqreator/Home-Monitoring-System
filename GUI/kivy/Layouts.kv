<TopPanelLayout@AnchorLayout>:
    anchor_x: 'left'
    anchor_y: 'top'
    padding: '48dp','36dp', '48dp', '24dp'

<TopPanelSubLayout@RelativeLayout>:
    size_hint_y: None
    height: '68dp'

<BottomPanelLayout@AnchorLayout>:
    pos: self.parent.pos
    anchor_x: 'left'
    anchor_y: 'bottom'
    padding: '48dp','24dp', '48dp', '24dp'

<BottomPanelSubLayout@RelativeLayout>:
    pos: self.parent.pos
    size_hint_y: None
    size: '0dp', '64dp'

<MenuLayout@GridLayout>:
    padding: '48dp','24dp', '48dp', '24dp'
    spacing: '24dp', '0dp'
    pos: '0dp', '112dp'
    size_hint_y: None
    size: '0dp', '376dp'
    cols: 3

<MenuSublayout@RelativeLayout>:
    size: self.parent.size
    pos: self.parent.pos

<ContentLayout@AnchorLayout>:
    anchor_y: 'top'
    padding: '0dp', '144dp', '0dp', '48dp'

<ContentSublayout@GridLayout>:
    size_hint: None, None
    size: '928dp', '360dp'
    rows: 1

<ButtonLayout@RelativeLayout>:
    size_hint_y: None
    height: '64dp'

<RecycledMaterialLayout@GridLayout>:
    cols: 3
    size_hint: None, None
    padding: '136dp', '16dp', '136dp', '16dp'







